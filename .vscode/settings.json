// 将设置放入此文件中以覆盖默认值和用户设置。
{
    "files.associations": {
        "*.js": "javascript",
        "*.vue": "vue"
    },
    "editor.fontSize": 16,
    "editor.mouseWheelZoom": true,
    "editor.renderWhitespace": "boundary",
    "eslint.enable": true,
    "eslint._legacyModuleResolve": true,
    "eslint.autoFixOnSave": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "vue",
        "html"
    ],
    "editor.tabSize": 4,
    "vsicons.presets.angular": false,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/*.js": true
    }
}